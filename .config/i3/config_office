set $mode_trans Window Opacity (+|-) +|-25, (0) 100
mode "$mode_trans" {
  bindsym Shift+plus exec /usr/bin/compton-trans -c +25
  bindsym minus exec /usr/bin/compton-trans -c -25
  bindsym plus exec /usr/bin/compton-trans -c 100
  # back to normal: Enter or Escape
  bindsym Return mode "default"
  bindsym Escape mode "default"
}
bindsym $mod+Shift+t mode "$mode_trans"

### Screenshots
# Print mode: xdgopen, rm, save
set $mode_print Printscreen (o) open, (r) remove, (s) save
mode "$mode_print" {
    bindsym o exec --no-startup-id xdg-open /tmp/scrot.png
    bindsym s exec --no-startup-id "mv /tmp/scrot.png /home/wayne/Pictures/screenshots/$(date +%Y%m%d_%H%M%S).png", mode "default"
    bindsym r exec --no-startup-id "rm /tmp/scrot.png", mode "default"

    # back to normal: Enter or Escape
    bindsym Return exec --no-startup-id "rm /tmp/scrot.png", mode "default"
    bindsym Escape exec --no-startup-id "rm /tmp/scrot.png", mode "default"
}
bindsym Print exec --no-startup-id "scrot -m /tmp/scrot.png", mode "$mode_print"

bindsym --release Shift+Print exec "scrot -s /tmp/scrot.png; 'xclip -se c -t image/png -i $f'"

set $mode_system System (l) lock, (e) logout, (r) reboot, (p/q/s) poweroff
mode "$mode_system" {
  bindsym l exec --no-startup-id i3exit lock, mode "default"
  bindsym e exec --no-startup-id i3exit logout, mode "default"
  #bindsym s exec --no-startup-id i3exit suspend, mode "default"
  #bindsym h exec --no-startup-id i3exit hibernate, mode "default"
  bindsym r exec --no-startup-id i3exit reboot, mode "default"
  bindsym p exec --no-startup-id i3exit shutdown, mode "default"
  bindsym q exec --no-startup-id i3exit shutdown, mode "default"
  bindsym s exec --no-startup-id i3exit shutdown, mode "default"

  # back to normal: Enter or Escape
  bindsym Return mode "default"
  bindsym Escape mode "default"
}
bindsym $mod+Shift+Escape mode "$mode_system"

