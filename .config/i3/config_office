# Start i3bar to display a workspace bar (plus the system information i3status
# finds out, if available)

bar {
  i3bar_command i3bar -t
  status_command SCRIPT_DIR=~/i3blocks/scripts i3blocks
  output $primary_output
  output $secondary_output
  position top
  strip_workspace_numbers yes
  font pango:MesloLGS Nerd Font Mono 10.5
  height 25
  colors {
    separator #928374
    background #28282800
    statusline #ebdbb2
    focused_workspace #689d6a #689d6a #28282800
    active_workspace #3c3836 #3c3836 #928374
    inactive_workspace #3c383600 #28282800 #928374
    urgent_workspace #cc241d #cc241d #ebdbb2

    binding_mode #458588 #458588 #00000000
  }
}

